{
  "name": "kmrl-backend",
  "version": "1.0.0",
  "description": "KMRL Train Induction System Backend API",
  "main": "dist/server-simple.js",
  "scripts": {
    "dev": "nodemon src/server-simple.ts",
    "dev:mock": "nodemon src/serverMock.ts",
    "build": "tsc",
    "start": "node dist/server-simple.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node src/prisma/seed.ts",
    "prisma:seed-enhanced": "ts-node prisma/seed-enhanced.ts",
    "test:job-cards": "ts-node test-job-cards.ts",
    "db:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "multer": "^1.4.5-lts.1",
    "@types/multer": "^1.4.12",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.12.1",
    "morgan": "^1.10.0",
    "mqtt": "^5.14.1",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "prisma": "^5.6.0",
    "redis": "^4.6.10",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/mqtt": "^0.0.34",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "kmrl",
    "train",
    "scheduling",
    "api",
    "express"
  ],
  "author": "KMRL Development Team",
  "license": "MIT"
}
